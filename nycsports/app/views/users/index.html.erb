
<h1>User Index Page</h1>
<h3>ADMIN ONLY</h3>

<ul>
	<% @users.each do |user| %>
		<li>
			<p><%= link_to "#{user['name']}", user_path(user)%></p>
			<%= link_to 'Edit User Profile', edit_user_path(user) %>
			<%= link_to 'Delete User', user_path(user), method: :delete %> 
			<br/> <br/>
		</li>
	<% end %>
</ul>
