<h1>Index Teams Page</h1>

<h3>ADMIN ONLY</h3>

<ul>
	<% @teams.each do |team| %>
		<li>
			<p><%= link_to "#{team['name']}", team_path(team)%></p>
			<%= link_to 'Edit Team', edit_team_path(team) %>
			<%= link_to 'Delete Team', team_path(team), method: :delete %> 
			<br/> <br/>
		</li>
	<% end %>
</ul>