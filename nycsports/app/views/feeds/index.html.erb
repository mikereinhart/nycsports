<h1>Index Feeds Page </h1>
<h3>ADMIN ONLY</h3>

<ul>
	<% @feeds.each do |feed| %>
		<li>
	 		<p><%= link_to "#{feed['name']}", feed_path(feed) %></p>
	 		<%= link_to 'Edit Feed', edit_feed_path(feed) %>
	 		<%= link_to "Delete Feed", feed_path(feed), method: :delete %>
		 	<br/> <br/>
		</li>
	<% end %>
</ul>